<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="SilviuCRM" transaction-type="RESOURCE_LOCAL">
		<class>silviuCRMClase.Angajat</class>
		<class>silviuCRMClase.Comanda</class>
		<class>silviuCRMClase.Contact</class>
		<class>silviuCRMClase.Email</class>
		<class>silviuCRMClase.Lead</class>
		<class>silviuCRMClase.LinieComanda</class>
		<class>silviuCRMClase.LinieOferta</class>
		<class>silviuCRMClase.LoginCreateAccount</class>
		<class>silviuCRMClase.Oferta</class>
		<class>silviuCRMClase.Operatiune</class>
		<class>silviuCRMClase.Produs</class>
		<class>silviuCRMClase.RaspunsTicket</class>
		<class>silviuCRMClase.Ticket</class>
		
		<exclude-unlisted-classes>false</exclude-unlisted-classes>
		<properties>
			<property name="eclipselink.jdbc.native-sql" value="false"/>
			<property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/SilviuCRM"/>
			<property name="javax.persistence.jdbc.user" value="postgres"/>
			<property name="javax.persistence.jdbc.password" value="postgres"/>
			<property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
		</properties>
	</persistence-unit>
</persistence>
